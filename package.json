{
  "name": "multi-tenant-rate-limiter",
  "version": "1.0.0",
  "description": "Production-ready multi-tenant rate limiting system with hierarchical policies and progressive throttling",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "dev:debug": "node --inspect=0.0.0.0:9229 -r ts-node/register src/index.ts",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:load": "k6 run tests/load/rate-limiter.k6.js",
    "test:client": "ts-node scripts/test-client.ts",
    "test:abuse": "ts-node scripts/test-abuse-detection.ts",
    "test:admin": "ts-node scripts/test-admin-api.ts",
    "lint": "eslint 'src/**/*.ts' 'tests/unit/**/*.ts' 'tests/integration/**/*.ts'",
    "format": "prettier --write 'src/**/*.ts' 'tests/unit/**/*.ts' 'tests/integration/**/*.ts'",
    "seed": "ts-node scripts/seed-policies.ts",
    "docker:up": "docker-compose -f infrastructure/docker-compose.yml up -d",
    "docker:down": "docker-compose -f infrastructure/docker-compose.yml down",
    "docker:logs": "docker-compose -f infrastructure/docker-compose.yml logs -f"
  },
  "keywords": [
    "rate-limiting",
    "multi-tenant",
    "redis",
    "token-bucket",
    "throttling"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.13.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^10.1.0",
    "mongoose": "^8.0.3",
    "prom-client": "^15.1.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.6",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  }
}
